{
    "name": "Twitter",
    "url": "https://api.twitter.com/1.1/search/tweets.json",
    "method": "GET",
    "api_type": "text",
    "path_to_results": [
        "statuses"
    ],
    "request": [
        {
            "type": "string",
            "required": true,
            "position": "body",
            "relevant": false,
            "api_name": "q"
        },
        {
            "type": "string",
            "required": false,
            "default_value": "en",
            "position": "body",
            "relevant": false,
            "api_name": "lang"
        },
        {
            "type": "string",
            "values": [
                "mixed",
                "recent",
                "popular"
            ],
            "required": false,
            "default_value": "recent",
            "position": "body",
            "relevant": false,
            "api_name": "result_type"
        },
        {
            "type": "int",
            "required": false,
            "default_value": "100",
            "position": "body",
            "relevant": false,
            "api_name": "count"
        },
        {
            "type": "string",
            "required": true,
            "position": "header",
            "relevant": false,
            "api_name": "Authorization"
        }
    ],
    "response": [
        {
            "type": "int64",
            "name": "id",
            "api_name": "id"
        },
        {
            "type": "string",
            "name": "date",
            "api_name": "created_at"
        },
        {
            "type": "string",
            "name": "text",
            "api_name": "text"
        },
        {
            "type": "int",
            "name": "retweet",
            "api_name": "retweet_count"
        },
        {
            "type": "int",
            "name": "favorite",
            "api_name": "favorite_count"
        },
        {
            "type": "string",
            "name": "lang",
            "api_name": "lang"
        }
    ]
}
